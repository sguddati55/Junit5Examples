 node {
    properties([
  parameters([
    string(name: 'SUT_NAME', defaultValue: 'Test', description: 'Which planet are we on?'),
  ]),
  pipelineTriggers([
    parameterizedCron('''
        * * * * * %SUT_NAME=Test;
        */2 * * * * %SUT_NAME=Dev
    ''')
  ])
])

        stage('Example') {
            
                echo "${params.SUT_NAME}"
                script { currentBuild.description = "${params.SUT_NAME}" }
            
        }
    

}
