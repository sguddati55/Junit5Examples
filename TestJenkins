 node {
    properties([
  parameters([
    string(name: 'SUT_NAME', defaultValue: 'Test', description: 'Which planet are we on?'),
  ]),
  pipelineTriggers([
    parameterizedCron('''
        * * * * * %SUT_NAME=Test;
        */2 * * * * %SUT_NAME=Dev
    ''')
  ])
])

 stage('clone') {
git clone 'https://github.com/sguddati55/Junit5Examples.git'
echo "pulled the code"
}
 
def props = readProperties  file:'$WORKSPACE/src/test/resources/envvariables.properties'
def Var1= props['slackTo']
echo "Var1=${Var1}"

        stage('Example') {
            
                echo "${env.emailTo}"
                echo "${env.slackTo}"
                script { currentBuild.description = "${params.SUT_NAME}" }
            
        }
    

}
